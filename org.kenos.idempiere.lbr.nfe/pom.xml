<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.kenos.idempiere</groupId>
		<artifactId>org.kenos.idempiere.lbr</artifactId>
		<version>6.2.0-SNAPSHOT</version>
		<relativePath>../pom.xml</relativePath>
	</parent>
	<artifactId>org.kenos.idempiere.lbr.nfe</artifactId>
	<packaging>eclipse-plugin</packaging>
	<repositories>
		<repository>
			<id>github-maven-repo</id>
			<url>https://raw.githubusercontent.com/KenosSGI/maven-repository/master/repository/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>local-maven-repo</id>
			<url>file://${project.basedir}/../local-maven-repo</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>
	<build>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-dependency-plugin</artifactId>
					<executions>
						<execution>
							<phase>validate</phase>
							<goals>
								<goal>copy</goal>
							</goals>
							<configuration>
								<artifactItems>
									<artifactItem>
										<groupId>local.nfebeans-canc-v101-sources</groupId>
										<artifactId>nfebeans-canc-v101-sources</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-canc-v101-sources.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-canc-v101</groupId>
										<artifactId>nfebeans-canc-v101</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-canc-v101.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-cce-v101-sources</groupId>
										<artifactId>nfebeans-cce-v101-sources</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-cce-v101-sources.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-cce-v101</groupId>
										<artifactId>nfebeans-cce-v101</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-cce-v101.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-dfe-v102-sources</groupId>
										<artifactId>nfebeans-dfe-v102-sources</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-dfe-v102-sources.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-dfe-v102</groupId>
										<artifactId>nfebeans-dfe-v102</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-dfe-v102.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-gen-v101-sources</groupId>
										<artifactId>nfebeans-gen-v101-sources</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-gen-v101-sources.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-gen-v101</groupId>
										<artifactId>nfebeans-gen-v101</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-gen-v101.jar</destFileName>
