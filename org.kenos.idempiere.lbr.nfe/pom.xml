										<destFileName>nfebeans-v9.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.siscomexbeans-v100-sources</groupId>
										<artifactId>siscomexbeans-v100-sources</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>siscomexbeans-v100-sources.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.siscomexbeans-v100</groupId>
										<artifactId>siscomexbeans-v100</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>siscomexbeans-v100.jar</destFileName>
									</artifactItem>
									<artifactItem>
