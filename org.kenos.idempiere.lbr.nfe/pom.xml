									<artifactItem>
										<groupId>local.nfebeans-manif-v101-sources</groupId>
										<artifactId>nfebeans-manif-v101-sources</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-manif-v101-sources.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-manif-v101</groupId>
										<artifactId>nfebeans-manif-v101</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-manif-v101.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-v6v-sources</groupId>
										<artifactId>nfebeans-v6v-sources</artifactId>
										<version>NOT_INFORMED</version>
										<destFileName>nfebeans-v6v-sources.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>local.nfebeans-v6v</groupId>
