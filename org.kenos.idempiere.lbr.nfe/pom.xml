									<artifactItem>
										<groupId>com.google.zxing</groupId>
										<artifactId>core</artifactId>
										<version>2.2</version>
										<destFileName>zxing-core-2.2.jar</destFileName>
									</artifactItem>
									<artifactItem>
										<groupId>com.google.zxing</groupId>
										<artifactId>javase</artifactId>
										<version>2.2</version>
										<destFileName>zxing-javase-2.2.jar</destFileName>
									</artifactItem>
								</artifactItems>
								<outputDirectory>lib</outputDirectory>
								<stripVersion>true</stripVersion>
								<overWriteReleases>true</overWriteReleases>
								<overWriteSnapshots>true</overWriteSnapshots>
							</configuration>
						</execution>
					</executions>
				</plugin>
			</plugins>
	</build>
</project>
